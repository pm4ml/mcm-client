{
  "name": "@pm4ml/mcm-client",
  "version": "3.8.0",
  "description": "Client service for Connection Manager API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Yevhen Kyriukha, ModusBox Inc.",
  "contributors": [
    "Yevhen Kyriukha <yevhen.kyriukha@modusbox.com>",
    "Murthy Kakarlamudi <murthy@modusbox.com>",
    "James Bush <jbush@mojaloop.io>"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/pm4ml/mcm-client.git"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint & npm run build & npm test"
    }
  },
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "jest --ci --reporters=default --reporters=jest-junit --env=node dist/test/unit",
    "test:int": "jest --ci --reporters=default --reporters=jest-junit --env=node src/test/integration --forceExit",
    "prepublishOnly": "tsc",
    "dep:check": "npx ncu -e 2",
    "dep:update": "npx ncu -u",
    "build": "tsc"
  },
  "dependencies": {
    "@mojaloop/sdk-standard-components": "19.16.5",
    "@xstate/inspect": "0.7.0",
    "ajv": "^8.17.1",
    "async-retry": "^1.3.3",
    "dotenv": "^16.4.5",
    "fast-json-patch": "^3.1.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.2",
    "node-forge": "^1.3.1",
    "node-vault": "^0.10.2",
    "tslib": "^2.8.1",
    "uuid": "^10.0.0",
    "ws": "^8.18.0",
    "xstate": "4.35.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.13.4",
    "@types/ws": "^8.5.14",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "hasky": "^3.0.2",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "mockdate": "^3.0.5",
    "nock": "^13.5.5",
    "npm-check-updates": "17.0.6",
    "openapi-response-validator": "^12.1.3",
    "typescript": "^5.5.4"
  }
}
