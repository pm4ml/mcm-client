{
  "name": "@pm4ml/mcm-client",
  "version": "3.0.0",
  "description": "Client service for Connection Manager API",
  "main": "src/index.js",
  "types": "dist/index.d.ts",
  "author": "Yevhen Kyriukha, ModusBox Inc.",
  "contributors": [
    "Yevhen Kyriukha <yevhen.kyriukha@modusbox.com>",
    "Murthy Kakarlamudi <murthy@modusbox.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/pm4ml/mcm-client.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint & npm test"
    }
  },
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "jest --ci --reporters=default --reporters=jest-junit --env=node src/test/unit",
    "test:int": "jest --ci --reporters=default --reporters=jest-junit --env=node src/test/integration --forceExit",
    "prepublishOnly": "tsc"
  },
  "dependencies": {
    "@mojaloop/sdk-standard-components": "github:mojaloop/sdk-standard-components#feat/IPROD-93",
    "ajv": "^8.11.0",
    "async-retry": "^1.3.3",
    "dotenv": "^16.0.0",
    "fast-json-patch": "^3.1.1",
    "js-yaml": "^4.1.0",
    "node-fetch": "^3.2.4",
    "node-vault": "^0.9.22",
    "uuid": "^8.3.2",
    "ws": "^8.6.0"
  },
  "devDependencies": {
    "@types/jest": "^27.5.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "hasky": "^3.0.2",
    "jest": "^28.1.0",
    "jest-junit": "^13.2.0",
    "mockdate": "^3.0.5",
    "nock": "^13.2.4",
    "openapi-response-validator": "^11.0.0",
    "typescript": "^4.6.4"
  }
}
